<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.myapplication.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_FirstFragment_to_ThirdFragment"
            app:destination="@id/ThirdFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_ninthFragment"
            app:destination="@id/ninthFragment" />
    </fragment>
    <fragment
        android:id="@+id/ThirdFragment"
        android:name="com.example.myapplication.ThirdFragment"
        android:label=""
        tools:layout="@layout/fragment_third" >
        <action
            android:id="@+id/action_ThirdFragment_to_FourthFragment"
            app:destination="@id/FourthFragment" />
    </fragment>
    <fragment

        android:id="@+id/FourthFragment"
        android:name="com.example.myapplication.FourthFragment"
        android:label="@string/fourth_fragment_label"
        tools:layout="@layout/fragment_fourth">

        <action

            android:id="@+id/action_FourthFragment_to_FirstFragment"

            app:destination="@id/FirstFragment" />

        <action

            android:id="@+id/action_FourthFragment_to_fifthFragment"

            app:destination="@id/fifthFragment" />

    </fragment>

    <fragment

        android:id="@+id/fifthFragment"

        android:name="com.example.myapplication.FifthFragment"

        android:label="@string/fifth_fragment_label">

        <action
            android:id="@+id/action_fifthFragment_to_FourthFragment"
            app:destination="@id/FourthFragment" />
        <action
            android:id="@+id/action_fifthFragment_to_sixthFragment"
            app:destination="@id/sixthFragment" />

    </fragment>

    <fragment
        android:id="@+id/sixthFragment"
        android:name="com.example.myapplication.SixthFragment"
        android:label="@string/sixth_fragment_label">
        <action
            android:id="@+id/action_sixthFragment_to_seventhFragment"
            app:destination="@id/seventhFragment" />
        <action
            android:id="@+id/action_sixthFragment_to_fourthFragment"
            app:destination="@id/FourthFragment" />
        <action
            android:id="@+id/action_sixthFragment_to_eighthFragment"
            app:destination="@id/eighthFragment" />
    </fragment>
    <fragment
        android:id="@+id/seventhFragment"
        android:name="com.example.myapplication.SeventhFragment"
        android:label="@string/seventh_fragment_label">
        <action
            android:id="@+id/action_seventhFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>

    <fragment
        android:id="@+id/eighthFragment"
        android:name="com.example.myapplication.EighthFragment"
        android:label="@string/eighth_fragment_label">
        <action
            android:id="@+id/action_eighthFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/ninthFragment"
        android:name="com.example.myapplication.NinthFragment"
        android:label="@string/ninth_fragment_label"
        tools:layout="@layout/fragment_ninth">
        <action
            android:id="@+id/action_ninthFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
</navigation>
